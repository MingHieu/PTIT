{
  "name": "vina-edu",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env.prod -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:dev:rm": "docker compose -f docker-compose.dev.yml --env-file .env.dev rm database -s -v",
    "db:dev:up": "docker compose -f docker-compose.dev.yml --env-file .env.dev up database -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "prisma:dev:generate": "dotenv -e .env.dev -- prisma generate",
    "prisma:dev:migrate": "dotenv -e .env.dev -- prisma migrate dev",
    "prisma:dev:studio": "dotenv -e .env.dev -- prisma studio",
    "prisma:dev:seed": "dotenv -e .env.dev -- prisma db seed",
    "db:prod:rm": "docker compose -f docker-compose.prod.yml --env-file .env.prod rm database -s -v",
    "db:prod:up": "docker compose -f docker-compose.prod.yml --env-file .env.prod up database -d",
    "db:prod:restart": "yarn db:prod:rm && yarn db:prod:up",
    "prisma:prod:generate": "dotenv -e .env.prod -- prisma generate",
    "prisma:prod:migrate": "dotenv -e .env.prod -- prisma migrate deploy",
    "prisma:prod:studio": "dotenv -e .env.prod -- prisma studio",
    "prisma:prod:seed": "dotenv -e .env.prod -- prisma db seed",
    "docker:prod:up": "docker compose -f docker-compose.prod.yml --env-file .env.prod up -d"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/websockets": "^10.4.7",
    "@prisma/client": "^6.0.1",
    "argon2": "^0.41.1",
    "aws-sdk": "^2.1692.0",
    "cache-manager": "5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-cli": "^8.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nest-aws-sdk": "^3.1.0",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "src/database/prisma/schema",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/database/prisma/seed.ts"
  }
}
